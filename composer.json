{
    "name": "appwilio/yakassa",
    "type": "library",
    "description": "Yandex.Kassa payments",
    "keywords": ["laravel", "payments", "yandex", "Яндекс", "Яндекс.Касса"],
    "license": "MIT",
    "authors": [
        {
            "name": "JhaoDa",
            "role": "Developer",
            "email": "jhaoda@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.1",
        "illuminate/http": "~5.3|^6.0|^7.0|^8.0",
        "illuminate/support": "~5.3|^6.0|^7.0|^8.0",
        "ext-json": "*",
        "ext-mbstring": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5"
    },
    "autoload": {
        "psr-4": {
            "Appwilio\\YaKassa\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Appwilio\\YaKassa\\Test\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "sort-packages": true
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
